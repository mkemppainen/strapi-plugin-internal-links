	},
	"peerDependencies": {
		"@strapi/strapi": "^4.5.6"
	},
	"author": {
		"name": "Webbio B.V.",
		"email": "info@webbio.nl",
		"url": "https://webbio.nl"
	},
	"maintainers": [
		{
			"name": "Webbio B.V.",
			"email": "info@webbio.nl",
			"url": "https://webbio.nl"
		}
	],
	"engines": {
		"node": ">=14.19.1 <=18.x.x",
		"npm": ">=6.0.0"
	},
