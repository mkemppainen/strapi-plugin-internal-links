		"kind": "plugin"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/webbio/strapi-plugin-internal-links.git"
	},
	"dependencies": {
		"@strapi/icons": "^1.4.1",
		"cheerio": "1.0.0-rc.12"
	},
