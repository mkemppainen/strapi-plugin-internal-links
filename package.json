{
	"name": "@webbio/strapi-plugin-internal-links",
	"version": "0.0.4",
	"description": "A custom field for Strapi that can create internal links",
	"scripts": {
		"develop": "tsc -w",
		"build": "tsc",
		"format": "prettier --write .",
		"prepublish": "tsc"
	},
	"strapi": {
